<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
   <mime-type type="application/x-gnome-saved-search">
     <sub-class-of type="text/xml"/>
     <_comment>Saved search</_comment>
     <glob pattern="*.savedSearch"/>
   </mime-type>

   <mime-type type="application/kraken-action">
     <_comment>Kraken action definition</_comment>
     <generic-icon name="text-x-generic"/>
     <sub-class-of type="text/plain"/>
     <glob pattern="*.kraken_action"/>
   </mime-type>
   <mime-type type="application/kraken-search-helper">
     <_comment>Kraken search-helper definition</_comment>
     <generic-icon name="text-x-generic"/>
     <sub-class-of type="text/plain"/>
     <glob pattern="*.kraken_search_helper"/>
   </mime-type>
</mime-info>
